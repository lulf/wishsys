User
    name Text
    password Text
    salt Text
    UniqueUser name
    deriving Typeable

Wishlist
    name Text

WishlistViewer
    listId WishlistId
    owner UserId
    guest UserId

Wish
    name Text
    imageUrl Text
    stores Text
    amount Int64
    bought Int64
    list WishlistId


